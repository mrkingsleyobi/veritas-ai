{
  "name": "deepfake-detection-platform",
  "version": "1.0.0",
  "description": "Deepfake detection platform with RUV profile fusion for enhanced content authenticity verification",
  "main": "src/index.js",
  "scripts": {
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/e2e",
    "test:performance": "jest tests/performance",
    "test:security": "jest tests/security",
    "test:property": "jest tests/property",
    "test:load": "jest tests/load",
    "test:contract": "jest tests/contract",
    "test:mutation": "jest tests/mutation",
    "test:fuzz": "jest tests/fuzz",
    "test:chaos": "jest tests/chaos",
    "test:penetration": "jest tests/penetration",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "lint": "eslint src tests",
    "lint:fix": "eslint src tests --fix",
    "lint:security": "eslint --config .eslintrc.security.js src/",
    "typecheck": "tsc --noEmit",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "benchmark": "node benchmarks/run.js",
    "benchmark:compare": "node benchmarks/compare.js",
    "build": "tsc",
    "start": "node src/server/index.js",
    "dev": "nodemon src/server/index.js",
    "security:test": "jest tests/security",
    "security:scan": "npx eslint --config .eslintrc.security.js src/ && npm audit",
    "k6:run": "k6 run tests/load/contentAuthenticator.stress.test.js"
  },
  "keywords": [
    "deepfake",
    "detection",
    "authenticity",
    "verification",
    "RUV",
    "profile",
    "fusion"
  ],
  "author": "Veritas AI Team",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^8.0.0",
    "eslint-plugin-security": "^3.0.1",
    "fast-check": "^4.3.0",
    "jest": "^29.0.0",
    "k6": "^0.0.0",
    "msw": "^2.11.6",
    "nodemon": "^3.0.0",
    "supertest": "^7.1.4"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "testMatch": [
      "**/tests/**/*.test.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ],
    "verbose": true
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-jaeger": "^2.2.0",
    "@opentelemetry/exporter-prometheus": "^0.207.0",
    "@opentelemetry/instrumentation-express": "^0.56.0",
    "@opentelemetry/instrumentation-http": "^0.207.0",
    "@opentelemetry/instrumentation-pg": "^0.60.0",
    "@opentelemetry/instrumentation-redis": "^0.56.0",
    "@opentelemetry/resource-detector-container": "^0.7.10",
    "@opentelemetry/sdk-node": "^0.207.0",
    "amqplib": "^0.10.9",
    "bcryptjs": "^3.0.3",
    "bullmq": "^5.63.0",
    "connect-redis": "^9.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.2",
    "express-validator": "^7.3.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.20.0",
    "mongoose": "^8.19.2",
    "mysql2": "^3.15.3",
    "passport": "^0.7.0",
    "passport-oauth2": "^1.8.0",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "redis": "^5.9.0",
    "sequelize": "^6.37.7",
    "speakeasy": "^2.0.0",
    "sqlite3": "^5.1.7",
    "uuid": "^13.0.0",
    "winston": "^3.18.3"
  }
}